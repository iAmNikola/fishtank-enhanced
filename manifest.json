{
    "name": "Fishtank Improver",
    "version": "2023.04.25",
    "description": "Adds new options for Fishtank Live",
    "author": "Sean",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["Fishtank.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icons/16.png",
        "19": "images/icons/19.png",
        "38": "images/icons/38.png",
        "48": "images/icons/48.png",
        "128": "images/icons/128.png"
      }
    },
    "icons": {
      "16": "images/icons/16.png",
      "19": "images/icons/19.png",
      "38": "images/icons/38.png",
      "48": "images/icons/48.png",
      "128": "images/icons/128.png"
    },
    "manifest_version": 2
}